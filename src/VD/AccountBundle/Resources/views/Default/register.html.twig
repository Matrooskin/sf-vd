{% extends "VDAccountBundle:Default:layout.html.twig" %}
{% block body %}
    {% if error %}
        <div>{{ error }}</div>
    {% endif %}
    <form action="{{ path('vd_account_register') }}" method="post" {{ form_enctype(form) }}>
        {{ form_widget(form) }}

        <input type="submit"/>
    </form>
{% endblock %}